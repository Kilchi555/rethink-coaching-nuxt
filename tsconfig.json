{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": ["@types/node", "nuxt"],
    "target": "es2020",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "#app": [".nuxt/types/app"],
      "~/*": ["./*"],
      "@/*": ["./*"]
    }
  },
  "include": ["nuxt.d.ts", "types", "nuxt.config.ts", "**/*.ts", "**/*.vue"]
}
